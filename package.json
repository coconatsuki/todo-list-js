{
  "name": "my-great-package",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:coconatsuki/todo-list-js.git",
  "author": "coconatsuki",
  "license": "MIT",
  "scripts": {
    "common:babel": "babel webpack.common.babel.js > webpack.common.js",
    "dev:babel": "yarn common:babel && babel webpack.dev.babel.js > webpack.dev.js",
    "prod:babel": "yarn common:babel webpack.prod.babel.js > webpack.prod.js",
    "dev": "yarn dev:babel && webpack-dev-server --port 8081 --config webpack.dev.js -d --progress",
    "prod": "yarn prod:babel && webpack --config webpack.prod.js --progress -p"
  },
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
