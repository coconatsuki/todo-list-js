{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": 1,
    "linebreak-style": 0,
    "no-unused-vars": 0,
    "no-restricted-syntax": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "no-loop-func": 0,
    "max-len": ["error", {"ignoreComments": true} ]
  }
}
